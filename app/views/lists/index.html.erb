<h3>List's name</h1>

  <%@lists.each do |list| %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, width: 400, crop: :fit %>')">
      <%= link_to list.name, list_path(list) %>
    </div>
  <% end %>
</div>
<%= link_to "Create a new list!", new_list_path %>
